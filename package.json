{
  "name": "my-fullstack-app",
  "version": "1.0.0",
  "description": "App for a react front end and express backend",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "start": "node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "mongodb-test": "jest --testPathIgnorePatterns /cypress /client",
    "e2e-local": "cypress run",
    "e2e": "cypress open"
  },
  "author": "Justin Johnson",
  "license": "MIT",
  "dependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@shelf/jest-mongodb": "^1.1.5",
    "axios": "^0.19.2",
    "babel-jest": "^24.9.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "cookie-parser": "^1.4.5",
    "cool-ascii-faces": "^1.3.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "formidable": "^1.2.2",
    "jest": "^24.9.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "material-ui": "^0.20.2",
    "mongoose": "^5.9.5",
    "morgan": "^1.10.0",
    "react-global-configuration": "^1.4.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-tap-event-plugin": "^3.0.3",
    "react-test-renderer": "^16.13.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "chance": "^1.1.4",
    "cypress": "^4.5.0",
    "mongo-unit": "^2.0.1",
    "nodemon": "^2.0.2"
  }
}
